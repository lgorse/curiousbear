<%= render 'facebook_friends_script' %>
<% @profile = @graph.get_connections("me", "friends", :fields => "name, id, picture")%>
<ul>Facebook Friends
<% @profile.reject{|profile| User.find_by_fb_id(profile["id"])}.each do |profile| %>
<% @friend_id = profile["id"] %>
<% @attr = {:id => nil, :fb_pic => profile["picture"]["data"]["url"], :fb_id => profile["id"], :name => profile["name"]} %>
<%= render "users/user_profile_summary", :user => User.new(@attr)%>

<% end %>
</ul>