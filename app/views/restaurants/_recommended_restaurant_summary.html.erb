<% set_summary_values_from_db(venue) %>
<li id="recommended_restaurant_summary">
	<div id="restaurant_summary_left">
		<ul>
	<li id="venue_name">
		<%= link_to venue.name, restaurant_path(venue.id) %>
		<%= render 'reviews/add_review' %>
	</li>
<li id = "venue_formatted_address"><%= @parsed_address[0]  %></li>
<li id = "venue_formatted_address"><%= @parsed_address[1]  %></li>
</ul>
<%= render 'restaurants/restaurant_reviewer_list', :venue => venue, :limit => 4 %>
	</div>
<div id = "restaurant_summary_right">

<div id="google_rating"><%= "Average rating: "+ average(venue).to_s %></div>
<div><%= pluralize(venue.reviews.count, "review") %></div>
 <a onclick='postToFeed(<%= @venue_json %>); return false;' href = "#" id="share_button">Request review</a>
 <p id='msg'></p>

</div>
</li>