<%= render 'restaurants/review_request_script' %>
<%= image_tag "powered-by-google-on-white.png" %>
<% unless @google_results.blank? %>
<ul>
	<%= render :partial => 'restaurants/google_restaurant_summary', :collection => @google_results_except_recommended, :as => :result %>
	
<% if @google_response["next_page_token"] %>
<%= link_to "next", :controller => "restaurants", :action => 'google_search', :next_token => @google_response["next_page_token"], :search => params[:search], :remote => true  %>
<% end %>
<%= raw @google_response["html_attributions"].join(", ")%>
</ul>
<% end %>

