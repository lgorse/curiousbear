
$(function(){
	$(".feedback_link, .fancybox").fancybox({
		type : 'iframe',
		//fitToView : false,
		autoSize : true,
		afterLoad : function(){
			
			block_off_default_clicks();
			$(".fancybox-iframe").contents().find(".feedback_form").submit(function(e){
				parent.$.fancybox.close();
			});
		}
	}).eq(0).click().stopPropagation();

});